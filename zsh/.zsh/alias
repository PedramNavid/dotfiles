#
# Files/Directories
#
alias cdp='cd $HOME/projects'
alias dh='dirs -v' 		# print dirstack
alias l='ls -AF'
alias ll='l -l'
alias la='ls -AF'


#
# File Edits
#
alias vim='nvim'
alias vzrc='vim ~/.zshrc'
alias vali='vim ~/.zsh/alias'
alias vexp='vim ~/.zsh/exports'

#
# Git
#
alias gst='git status'
alias gc='git commit -v'
alias gd='git diff'
alias gp='git push'
alias gpsup='git push --set-upstream origin $(git_current_branch)'


